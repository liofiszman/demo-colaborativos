<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="Controllers.TurnosController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Button text="X" onAction="#backToHome"/>

    <Label text ="Buscar turnos" fx:id="buscadorTitle"/>
    <Label/>
    <Label text ="Patente"/>
    <TextField fx:id="buscadorTextField" promptText="patente..." />
    <Button text="Buscar" onAction="#onTurnosPatenteClick"/>

</VBox>

